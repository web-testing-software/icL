@startuml

!include uml_style.iuml
title RichBlock.castNow()

start

:vt = value1.type();
:newContext = nullptr;

if (vt == Bool) then (Да)
	-[#blue]->
	:newContext =\nnew BooleanBlock();
else if (vt == Int) then (Да)
	-[#blue]->
	:newContext =\nnew IntBlock();
else if (vt == Double) then (Да)
	-[#blue]->
	:newContext =\nnew DoubleBlock();
else if (vt == String) then (Да)
	-[#blue]->
	:newContext =\nnew StringBlock();
else if (vt == StringList) then (Да)
	-[#blue]->
	:newContext =\nnew ListBlock();
else (Нет)
	-[#red]->
	:ошибка\nсемантики>
endif

if (newContext != nullptr) then (Да)
	-[#blue]->
	:newContext.frag1 = frag1;
	:newContext.frag2 = frag2;
	:newContext.value1 = value1;
	:newContext.oprationType = operationType;
	:newContext.casted = true;
else (Нет)
	-[#red]->
endif

:return newContext;

stop


@enduml