          String=""
          attribute=""
          context="#stay" />

        <RegExpr
          String=""
          attribute=""
          context="" />

        <RegExpr
          String=""
          attribute=""
          context="#pop" /> -->

    <itemDatas>
      <itemData name="Comment"      defStyleNum="dsComment"       spellChecking="false"/>
      <itemData name="Keyword"      defStyleNum="dsKeyword"       spellChecking="false" bold="1"/>
      <itemData name="Numeric"      defStyleNum="dsFloat"         spellChecking="false"/>
      <itemData name="Punctuation"  defStyleNum="dsNormal"        spellChecking="false"/>
      <itemData name="Illegal"      defStyleNum="dsError"         spellChecking="false"/>
      <itemData name="MetaType"     defStyleNum="dsDataType"      spellChecking="false"/>
      <itemData name="MetaClass"    defStyleNum="dsBuiltIn"       spellChecking="false"/>
      <itemData name="Property"     defStyleNum="dsAttribute"     spellChecking="false"/>
      <itemData name="CodeLiteral"  defStyleNum="dsPreprocessor"  spellChecking="false"/>
      <itemData name="Modifier"     defStyleNum="dsControlFlow"   spellChecking="false"/>
      <itemData name="Method"       defStyleNum="dsFunction"      spellChecking="false"/>
      <itemData name="LVariable"    defStyleNum="dsVariable"      spellChecking="false"/>
      <itemData name="GVariable"    defStyleNum="dsConstant"      spellChecking="false"/>
      <itemData name="String"       defStyleNum="dsString"        spellChecking="true"/>
      <itemData name="Function"     defStyleNum="dsFunction"      spellChecking="false"/>
      <itemData name="RegEx"        defStyleNum="dsSpecialString" spellChecking="false"/>
      <itemData name="Struct"       defStyleNum="dsExtension"     spellChecking="false"/>
      <itemData name="Enum"         defStyleNum="dsOthers"        spellChecking="false"/>
      <itemData name="Type"         defStyleNum="dsDataType"      spellChecking="false"/>
      <itemData name="Operator"     defStyleNum="dsOperator"      spellChecking="false"/>
    </itemDatas>

  </highlighting>

  <general>
    <comments>
      <comment name="singleLine" start="``" />
      <comment name="multiLine" start="```" end="````" />
    </comments>
  </general>

</language>